   title="proFIA: A data preprocessing workflow for Flow Injection Analysis coupled to High-Resolution Mass Spectrometry",
   journal="Bioinformatics",
   year="2017",
   volume="",
   pages="",
   url="",
   textVersion = paste("Delabriere, A., Hohenester, U., Colsch, B., Junot, C., Fenaille, F., Thevenot, E.",
                       "2017.",
                       "proFIA: A data preprocessing workflow for Flow Injection Analysis coupled to High-Resolution Mass Spectrometry",
